{
    "name": "datenkraken/backend",
    "type": "project",
    "description": "The backend for the Datenkraken project.",
    "keywords": [
        "dataprocessing",
        "backend",
        "laravel"
    ],
    "license": "MIT",
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/Datenkraken/laravel-mongodb-passport"
		}
	],
    "require": {
        "php": "^7.4",
        "designmynight/laravel-mongodb-passport": "dev-master#137b22750cd28d76c7b227ed2ebf3d899250863c",
        "fideloper/proxy": "^4.4",
        "jenssegers/mongodb": "^3.6",
        "kg-bot/laravel-localization-to-vue": "^1.6",
        "laravel/framework": "^6.2",
        "laravel/passport": "^9.3",
        "laravel/tinker": "^2.4",
        "nuwave/lighthouse": "^4.16",
        "spatie/laravel-cors": "^1.6",
        "spatie/laravel-csp": "^2.5"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.9",
        "laravel/ui": "^1.2",
        "mockery/mockery": "^1.4",
        "mxl/laravel-job": "^1.1",
        "nunomaduro/collision": "^3.0",
        "nunomaduro/phpinsights": "^1.14",
        "phpunit/phpunit": "^9.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
